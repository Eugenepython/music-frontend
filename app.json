{
  "expo": {
    "name": "my-music-game",
    "slug": "my-music-game",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "mymusicgame",
    "extra": {
      "eas": {
        "projectId": "0e5c71ac-6688-44b1-a03b-a224883e64d4"
      }
    },
    "android": {
      "package": "com.eugene1981.mymusicgame"
    }
  },
  "assetBundlePatterns": [
    "**/*"
  ],
  "ios": {
    "supportsTablet": true
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/icon.png",
      "backgroundColor": "#ffffff"
    },
    "package": "com.eugene1981.mymusicgame"
  },
  "web": {
    "favicon": "./assets/icon.png"
  },
  "plugins": [
    "expo-secure-store"
  ],
  "extra": {
    "eas": {
      "projectId": "0e5c71ac-6688-44b1-a03b-a224883e64d4"
    }
  }
}
